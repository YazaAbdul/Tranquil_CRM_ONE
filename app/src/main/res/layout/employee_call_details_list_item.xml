<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/mainDetailsLLID"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_35sdp"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="6"
        tools:ignore="UselessParent">

        <View
            android:layout_width="0.7dp"
            android:layout_height="match_parent"
            android:background="@color/black"/>

        <LinearLayout
            android:layout_width="@dimen/_80sdp"
            android:layout_height="match_parent"
            android:padding="@dimen/_1sdp"
            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="vertical">

            <TextView
                android:id="@+id/customerNameTVID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CUSTOMER NAME"
                android:maxLines="1"
                android:textSize="@dimen/_8ssp"
                android:textColor="@color/black"
                tools:ignore="HardcodedText,RtlSymmetry" />

            <TextView
                android:id="@+id/mobileTVID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="MOBILE NUMBER"
                android:textColor="@color/black"
                android:textSize="@dimen/_8ssp"
                tools:ignore="HardcodedText,RtlSymmetry" />



        </LinearLayout>




        <View
            android:layout_width="0.7dp"
            android:layout_height="match_parent"
            android:background="@color/black"/>


        <View
            android:layout_width="0.7dp"
            android:layout_height="match_parent"
            android:background="@color/black"
            android:visibility="gone"/>


        <TextView
            android:id="@+id/callStartTVID"
            android:layout_width="@dimen/_60sdp"
            android:layout_height="match_parent"
            android:text="CALL START"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_weight="1"
            android:textSize="@dimen/_8ssp"
            android:textColor="@color/black"
            android:paddingStart="@dimen/_5sdp"
            tools:ignore="HardcodedText,RtlHardcoded,RtlSymmetry" />

        <View
            android:layout_width="0.7dp"
            android:layout_height="match_parent"
            android:background="@color/black"/>


        <TextView
            android:id="@+id/callEndTVID"
            android:layout_width="@dimen/_60sdp"
            android:layout_height="match_parent"
            android:text="CALL END"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_weight="1"
            android:textSize="@dimen/_8ssp"
            android:textColor="@color/black"
            tools:ignore="HardcodedText,RtlHardcoded" />

        <View
            android:layout_width="0.7dp"
            android:layout_height="match_parent"
            android:background="@color/black"/>


        <TextView
            android:id="@+id/timeInSecTVID"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="match_parent"
            android:text="TIME IN SEC"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_weight="1"
            android:paddingStart="@dimen/_5sdp"
            android:textSize="@dimen/_8ssp"
            android:textColor="@color/black"
            tools:ignore="HardcodedText,RtlSymmetry" />

        <View
            android:layout_width="0.7dp"
            android:layout_height="match_parent"
            android:background="@color/black"/>

        <RelativeLayout
            android:id="@+id/nextCallRLID"
            android:layout_width="@dimen/_70sdp"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <TextView
                android:id="@+id/nextCallGapTVID"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:paddingStart="@dimen/_5sdp"
                android:text="NEXT CALL TIME GAP"
                android:textColor="@color/black"
                android:textSize="@dimen/_8ssp"
                tools:ignore="HardcodedText,RtlHardcoded,RtlSymmetry" />


        </RelativeLayout>


        <View
            android:layout_width="0.7dp"
            android:layout_height="match_parent"
            android:background="@color/black"/>


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.7dp"
        android:background="@color/black"/>


</LinearLayout>